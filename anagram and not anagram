#include<stdio.h>
#include<string.h>
int main()
{
    char str1[20],str2[20];
    int len,n1,n2,i,j,count=0,count1=0;
    gets(str1);
    gets(str2);
    n1 = strlen(str1);
    n2 = strlen(str2);
    if(n1 == n2)
    {
        len = n1;
        for(i=0; i<len; i++)
        {
            count=0;
            for(j=0; j<len; j++)
            {
                if(str1[i] == str2[j])
                {
                    count=1;
                    break;
                }
            }
            if(count== 0)
            {
                count1=1;
                break;
            }
        }
        if(count1==1)
        printf("\nNOT ANAGRAM");
        else
        printf("\nANAGRAM");
    }
    return 0;
}
